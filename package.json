{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "babel": {
      "compileAsTests": [
        "test/helpers/**/*"
      ]
    },
    "files": [
      "test/eslint-config-canonical/**/*"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint-plugin-ava": "^13.1.0",
    "eslint-plugin-canonical": "^2.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-jsonc": "^1.7.0",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-modules-newline": "0.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "eslint-plugin-yml": "^0.10.1",
    "typescript": "^4.4.4",
    "yaml-eslint-parser": "^0.4.1"
  },
  "description": "Canonical ESLint Shareable Config",
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.16.0",
    "@babel/register": "^7.16.0",
    "ava": "^3.15.0",
    "eslint": "^8.1.0",
    "husky": "^4.3.8",
    "jsonlint": "^1.6.3",
    "semantic-release": "^18.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run test"
    }
  },
  "keywords": [
    "eslint",
    "config",
    "canonical"
  ],
  "license": "BSD-3-Clause",
  "main": "./eslintrc.js",
  "name": "eslint-config-canonical",
  "peerDependencies": {
    "eslint": "^8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/eslint-config-canonical"
  },
  "scripts": {
    "format": "jsonlint --sort-keys --in-place --indent '  ' ./configurations/eslintrc.json && echo '' >> ./configurations/eslintrc.json && jsonlint --sort-keys --in-place --indent '  ' ./configurations/lodash.json && echo '' >> ./configurations/lodash.json && jsonlint --sort-keys --in-place --indent '  ' ./configurations/flowtype.json && echo '' >> ./configurations/flowtype.json",
    "generate-typescript-compatibility-rules": "node bin/generate-typescript-compatibility-rules.js > configurations/typescript-compatibility.json",
    "lint": "eslint --report-unused-disable-directives .",
    "test": "ava --verbose --serial"
  },
  "version": "1.0.0"
}
